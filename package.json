{
  "name": "verbise",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t verbise-app .",
    "docker:run": "docker run -d -p 8077:80 --name verbise-container --restart unless-stopped verbise-app",
    "docker:stop": "docker rm -f verbise-container || exit 0",
    "docker:clean": "docker rmi verbise-app || exit 0",
    "docker:deploy": "npm run docker:stop && npm run docker:clean && npm run docker:build && npm run docker:run",
    "docker:logs": "docker logs -f verbise-container"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "element-plus": "^2.9.1",
    "events": "^3.3.0",
    "eventsource-polyfill": "^0.9.6",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "mobx": "^6.13.5",
    "nanoid": "^5.0.9",
    "normalize.css": "^8.0.1",
    "opentype.js": "^1.3.4",
    "pinia": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.0-beta.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@types/events": "^3.0.3",
    "@types/is-hotkey": "^0.1.10",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.8",
    "fs-extra": "^11.3.0",
    "less": "^4.2.1",
    "prettier": "^3.5.0",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.1",
    "vite-plugin-chunk-split": "^0.5.0"
  }
}
